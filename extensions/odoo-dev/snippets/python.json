{   
    "Odoo Model": {
      "prefix": "Class",
      "body": [
        "class ${1:ModelName}(models.Model):",
        "    _name = '${2:model.name}'",
        "    _description = '${3:Description}'",
        "",
        "    ${4:field_name} = fields.${5:Char}('${6:Field Label}')"
      ],
      "description": "Odoo Model boilerplate"
    },
    "Create":{
      "prefix": ["create", "CREATE"],
      "body": [
        "@api.model_create_multi",
        "def create(self, vals_list):",
          "    // Your Logic here",
          "    return super().create(vals_list)"
      ]
    },
    "Write": {
      "prefix": ["write", "WRITE"],
      "body": [
        "def write(self, values):",
        "    // Your Logic here",
        "    res = super().write(values)",
        "    return res"
      ]
    },
    "Unlink": {
      "prefix": ["unlink", "UNLINK"],
      "body": [
        "def unlink(self):",
        "    // Your Logic here",
        "    res = super().unlink()",
        "    return res"
      ]
    },
    "Onchange": {
      "prefix": ["onchange", "ONCHANGE"],
      "body": [
        "@api.onchange('field_name')",
        "def _onchange_field_name(self):",
        "    // Your Logic here"
      ]
    },
    "Compute": {
      "prefix": ["compute", "Compute", "depends", "Depends"],
      "body": [
        "@api.depends('field_name')",
        "def _compute_field_name(self):",
        "    for rec in self:",
        "    // Your Logic here"
      ]
    },
    "Constraints": {
      "prefix": ["Constraints", "constraints", "check", "Check"],
      "body": [
       "@api.constrains('field_name')",
       "def _constrains_field_name(self):",
       "    // Your Logic here"
      ]
    }
}
  