    "tab.activeBackground": "#1E1F22",
    "tab.activeForeground": "#DFE1E5",
    "tab.activeBorder": "#9E6F70",
    "tab.inactiveBackground": "#2B2D30",
    "tab.inactiveForeground": "#6F737A",
    "tab.border": "#1E1F22",

    // Title Bar
    "titleBar.activeBackground": "#2B2D30",
    "titleBar.activeForeground": "#DFE1E5",
    "titleBar.inactiveBackground": "#393B40",
    "titleBar.inactiveForeground": "#6F737A",
    "titleBar.border": "#1E1F22",

