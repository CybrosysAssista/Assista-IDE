    "panel.background": "#2B2D30",
    "panel.border": "#1E1F22",
    "panelTitle.activeBorder": "#9E6F70",
    "panelTitle.activeForeground": "#DFE1E5",
    "panelTitle.inactiveForeground": "#6F737A",

